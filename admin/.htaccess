RewriteEngine On

# Remove .php extension from URLs for specific files
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^activity_login$ activity_login.php [L,QSA]
RewriteRule ^ms_account$ ms_account.php [L,QSA]

# Optionally, redirect .php URLs to remove the extension
RewriteCond %{THE_REQUEST} \s/+(activity_login|ms_account)\.php [NC]
RewriteRule ^ /%1 [R=301,L]
